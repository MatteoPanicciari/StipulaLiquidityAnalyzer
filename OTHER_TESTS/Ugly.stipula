/*
Non Liquid
Non w1-separate liquid
Non w2-separate liquid
*/

stipula Ugly {
    assets w1, w2
    fields x

    agreement(Mark, Sam) {
        Mark: x
    } => @Q0

    @Q0 Mark: get()[u] {
        u -o w2
    } => @Q1

    @Q1 Sam: shift()[] {
        w1 -o Sam
        w2 -o w1
    } => @Q1

    @Q1 Sam: end()[] {} => @Q2
}