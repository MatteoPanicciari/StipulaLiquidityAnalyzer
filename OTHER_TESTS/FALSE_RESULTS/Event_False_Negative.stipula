/* @Q2 is unreachable => h1 cannot be filled */

stipula Event_False_Negative {
    assets h1, h2
    fields x
    agreement (Part) {
        Part : x
    } => @Q0

    @Q0 Part : func()[u]{
        u -o h2

        now + 1D >> @Q1 {} => @Q2
        now >> @Q2 {
            h2 -o h1
        } => @Q2
    } => @Q1

    @Q2 Part : end()[]{
        h2 -o Part
    } => @Q3
}